#include "Etat1.h"

Etat1::~Etat1() {
}

bool Etat1::transition(Automate &automate, Symbole *s){
	switch(*s) {
	case NOMBRE:
	case VARIABLE:
	case EXPR:
		automate.decalage(s, new Etat1);
		break;
	case OUVREPAR:
		automate.decalage(s, new Etat1);
		break;
	}
	return false;
}

