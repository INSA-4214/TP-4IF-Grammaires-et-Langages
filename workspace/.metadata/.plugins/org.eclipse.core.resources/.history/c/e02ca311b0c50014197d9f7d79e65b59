#ifndef ETAT_H_
#define ETAT_H_

#include <boost/algorithm/string.hpp>

class Etat {
public:
	Etat(boost::string name);
	virtual ~Etat();
	void print() const;
	virtual bool transition(Automate & automate, Symbole *s) = 0;
};

#endif /* ETAT_H_ */
